#include<iostream> // problem number =07; 
using namespace std;
int prime(long int z)
{
    for(int j=2;j<=z/2;j++)
        if(z%j==0)
            return 0;
        return 1;
}
int main()
{
    int count=0,ele;long int i;
    cout<<"Enter=";cin>>ele;
    for(i=2;i<=9999999999;i++)
    {
        if(prime(i))
            count++;
        if(count==ele)
        {
        cout<<i;
        break;
        }
    }
}
int main() //problem number=06;
{
    int sum3=0,sum2=0,sum1=0,n;
    cout<<"Enter the nth number=";cin>>n;
    sum1=n*(n+1)*(2*(n+1))/2;
    cout<<"First sum="<<sum1<<endl;
    sum2=n*(n+1)/2;
    sum3=(sum2*sum2);
    cout<<"Sum two="<<sum3<<endl;
    cout<<"The difference between both of them="<<sum3-sum1<<endl;
}
